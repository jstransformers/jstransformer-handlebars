{
  "name": "jstransformer-handlebars",
  "version": "1.2.0",
  "description": "Handlebars support for JSTransformers.",
  "keywords": [
    "jstransformer",
    "handlebars"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "test-jstransformer": "^1.1.0",
    "xo": "^0.60.0"
  },
  "scripts": {
    "test": "test-jstransformer",
    "lint": "xo --space=2 --no-semicolon index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jstransformers/jstransformer-handlebars.git"
  },
  "author": {
    "name": "JSTransformers Team",
    "url": "http://github.com/orgs/jstransformers/people"
  },
  "license": "MIT",
  "xo": {
    "rules": {
      "unicorn/prefer-module": 0,
      "logical-assignment-operators": 0
    }
  }
}
